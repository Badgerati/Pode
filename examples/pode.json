{
    "web": {
        "static": {
            "defaults": [
                "index.html",
                "default.html"
            ],
            "cache": {
                "enable": true,
                "maxAge": 15,
                "include": [
                    "*.jpg"
                ]
            }
        },
        "errorPages": {
            "showExceptions": true,
            "strictContentTyping": true,
            "default": "application/html",
            "routes": {
                "/john": "application/json"
            }
        }
    },
    "server": {
        "fileMonitor": {
            "enable": true,
            "showFiles": true
        }
    }
}